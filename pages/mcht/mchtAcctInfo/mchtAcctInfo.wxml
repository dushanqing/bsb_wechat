                              <text wx:if="{{ index === 2 }}">{{ item.path }}</text>
                            </view>
                  </view> -->

  <!-- </view> -->

  <view class="weui-btn-area">
    <button class="weui-btn" type="primary" bindtap="mchtAcctNextStep">下一步</button>
  </view>
</view>
</view>